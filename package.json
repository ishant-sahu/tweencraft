{
  "homepage": "https://ishant-sahu.github.io/chimpoon",
  "name": "zoom-ui-collections",
  "version": "1.0.0",
  "description": "Repository for zoom collections",
  "main": "index.js",
  "repository": "https://bitbucket.org/rivigotech/zoom-ui-collections/",
  "author": "Ishant Sahu",
  "license": "MIT",
  "private": false,
  "scripts": {
    "predeploy": "npm run prod:build-client",
    "deploy": "gh-pages -d public",
    "lint": "eslint --cache --format codeframe --ext js,jsx src",
    "start": "cross-env NODE_ENV=development SSR=false webpack-dev-server -d --inline --config webpack.client.js ",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "dev": "cross-env NODE_ENV=development SSR=true npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js",
    "dev:build-client": "webpack --config webpack.client.js",
    "prod": "cross-env NODE_ENV=production SSR=true npm-run-all --parallel prod:*",
    "prod:build-server": "webpack --config webpack.server.js",
    "prod:build-client": "webpack --config webpack.client.js",
    "build": "npm-run-all --parallel build:*",
    "build:client": "webpack --config webpack.client.js",
    "build:server": "webpack --config webpack.server.js",
    "build:run-server": "node build/bundle.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@hot-loader/react-dom": "^16.13.0",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "connected-react-router": "^6.8.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.20",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.0.0",
    "styled-components": "^5.0.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^3.4.2",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  }
}
